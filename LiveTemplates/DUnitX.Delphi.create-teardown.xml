<?xml version="1.0" encoding="utf-8" ?>
<codetemplate xmlns="http://schemas.borland.com/Delphi/2005/codetemplates" version="1.0.0">
	<template name="dxtd" invoke="auto">
		<description>Create Teardown procedure</description>
		<author>DUnitX Group</author>
        <point name="procedurename">
            <text>
                Teardown
            </text>
            <hint>
                Name of the procedure
            </hint>
        </point>
		<code language="Delphi" context="memberdecl" delimiter="|" >
			<![CDATA[[Teardown]
procedure |procedurename|;|end|]]>
		</code>
	</template>
</codetemplate>
